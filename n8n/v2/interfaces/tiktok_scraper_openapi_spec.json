      }
    },
    "/acts/clockworks~free-tiktok-scraper/runs": {
      "post": {
        "operationId": "runs-sync-clockworks-free-tiktok-scraper",
        "x-openai-isConsequential": false,
        "summary": "Executes an Actor and returns information about the initiated run in response.",
        "tags": [
          "Run Actor"
        ],
        "requestBody": {
          "required": true,
