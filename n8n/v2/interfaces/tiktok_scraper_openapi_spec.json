    "/acts/clockworks~free-tiktok-scraper/run-sync": {
      "post": {
        "operationId": "run-sync-clockworks-free-tiktok-scraper",
        "x-openai-isConsequential": false,
        "summary": "Executes an Actor, waits for completion, and returns the OUTPUT from Key-value store in response.",
        "tags": [
          "Run Actor"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
